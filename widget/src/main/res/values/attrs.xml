<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomBarItem">
        <!--默认状态下的图标-->
        <attr name="bbIconNormal" format="reference" />
        <!--选中状态下的图标-->
        <attr name="bbIconSelected" format="reference" />
        <!--底部文字-->
        <attr name="bbItemText" format="string" />
        <!--文字大小-->
        <attr name="bbItemTextSize" format="dimension" />
        <!--默认状态下的文字颜色-->
        <attr name="bbTextColorNormal" format="color" />
        <!--选中状态下的文字颜色-->
        <attr name="bbTextColorSelected" format="color" />

        <!--文字和图标的顶部距离-->
        <attr name="bbItemMarginTop" format="dimension" />

        <!--是否开启触摸背景效果-->
        <attr name="bbOpenTouchBg" format="boolean" />
        <!--设置触摸背景-->
        <attr name="bbTouchDrawable" format="reference" />

        <!--设置图标的宽度-->
        <attr name="bbIconWidth" format="dimension" />
        <!--设置图标的高度-->
        <attr name="bbIconHeight" format="dimension" />

        <!--设置BottomBarItem的padding-->
        <attr name="bbItemPadding" format="dimension" />

        <!--设置未读数字体大小-->
        <attr name="bbUnreadTextSize" format="dimension" />
        <!--设置未读数字体颜色-->
        <attr name="bbUnreadTextColor" format="reference" />
        <!--设置未读数背景色-->
        <attr name="bbUnreadTextBg" format="reference" />

        <!--设置提示消息字体大小-->
        <attr name="bbMsgTextSize" format="dimension" />
        <!--设置提示消息字体颜色-->
        <attr name="bbMsgTextColor" format="reference" />
        <!--设置提示消息背景-->
        <attr name="bbMsgTextBg" format="reference" />

        <!--设置提示点背景-->
        <attr name="bbNotifyPointBg" format="reference" />

        <!--设置未读数组阈值 大于阈值的数字将显示为 n+ n为设置的阈值-->
        <attr name="bbUnreadThreshold" format="integer" />
    </declare-styleable>

    <declare-styleable name="BottomBarLayout">

        <attr name="bbSmoothScroll" format="boolean" />

    </declare-styleable>

    <declare-styleable name="ShapeImageView">
        <attr name="sivShape" format="integer">
            <enum name="circle" value="1" />
            <enum name="rectangle" value="2" />
            <enum name="arc" value="3" />
        </attr>
        <attr name="sivBorderWidth" format="dimension" />
        <attr name="sivBorderColor" format="color" />
        <attr name="sivRoundRadius" format="dimension" />
        <attr name="sivLeftTopRadius" format="dimension" />
        <attr name="sivRightTopRadius" format="dimension" />
        <attr name="sivRightBottomRadius" format="dimension" />
        <attr name="sivLeftBottomRadius" format="dimension" />
        <attr name="sivOnlyDrawBorder" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircleProgressView">
        <attr name="cpvNormalColor" format="color" />
        <attr name="cpvReachColor" format="color" />
        <attr name="cpvTextColor" format="color" />
        <attr name="cpvTextSize" format="dimension" />
        <attr name="cpvTextOffset" format="dimension" />
        <attr name="cpvNormalSize" format="dimension" />
        <attr name="cpvReachSize" format="dimension" />
        <attr name="cpvRadius" format="dimension" />
        <attr name="cpvTextVisible" format="boolean" />
        <attr name="cpvStartArc" format="integer" />
        <attr name="cpvTextSkewX" format="dimension" />
        <attr name="cpvTextPrefix" format="string" />
        <attr name="cpvTextSuffix" format="string" />
        <attr name="cpvInnerBackgroundColor" format="color" />
        <attr name="cpvStyle" format="enum">
            <enum name="Normal" value="0" />
            <enum name="FillInner" value="1" />
            <enum name="FillInnerArc" value="2" />
        </attr>
        <attr name="cpvInnerProgressColor" format="color" />
        <attr name="cpvInnerPadding" format="dimension" />
        <attr name="cpvOuterColor" format="color" />
        <attr name="cpvOuterSize" format="dimension" />
        <attr name="cpvReachCapRound" format="boolean" />
    </declare-styleable>

    <declare-styleable name="FlowLayout">

        <attr name="flGravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>

    </declare-styleable>

    <declare-styleable name="TagLayout">

        <attr name="tlMaxSelect" format="integer" />

    </declare-styleable>


    <declare-styleable name="RecyclerTabLayout">
        <attr name="tabIndicatorMode" format="enum">
            <enum name="line_square" value="0" />
            <enum name="line_round" value="1" />
            <enum name="triangle_up" value="2" />
            <enum name="triangle_down" value="3" />
            <enum name="point" value="4" />
            <enum name="resource" value="5" />
        </attr>
        <attr name="tabIndicatorColor" format="color" />
        <attr name="tabIndicatorHeight" format="dimension" />
        <attr name="tabIndicatorWidth" format="dimension" />
        <attr name="tabIndicatorMarginVertical" format="dimension" />
        <attr name="tabIndicatorResource" format="reference" />

        <!-- TAB背景 -->
        <attr name="tabBackground" format="reference" />

        <attr name="tabTextAppearance" format="reference" />
        <attr name="tabSelectedTextColor" format="color" />

        <attr name="tabOnScreenLimit" format="integer" />
        <attr name="tabMinWidth" format="dimension" />
        <attr name="tabMaxWidth" format="dimension" />

        <attr name="tabPaddingStart" format="dimension" />
        <attr name="tabPaddingTop" format="dimension" />
        <attr name="tabPaddingEnd" format="dimension" />
        <attr name="tabPaddingBottom" format="dimension" />
        <attr name="tabPadding" format="dimension" />

        <!-- 是否可以滑动切换 -->
        <attr name="tabScrollEnabled" format="boolean" />

    </declare-styleable>

    <declare-styleable name="WheelView">
        <!--文字大小-->
        <attr name="wvTextSize" format="dimension" />
        <!--是否自适应文字大小以完全显示过长的文字-->
        <attr name="wvAutoFitTextSize" format="boolean" />
        <!--文字对齐方式-->
        <attr name="wvTextAlign" format="enum">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <!--文字距离边界的外边距-->
        <attr name="wvTextBoundaryMargin" format="dimension" />
        <!--未选中文字颜色-->
        <attr name="wvNormalItemTextColor" format="color" />
        <!--被选中的文字颜色-->
        <attr name="wvSelectedItemTextColor" format="color" />
        <!--行间距-->
        <attr name="wvLineSpacing" format="dimension" />
        <!--数据为Integer类型时是否需要转换-->
        <attr name="wvIntegerNeedFormat" format="boolean" />
        <!--Integer类型转换格式-->
        <attr name="wvIntegerFormat" format="string" />

        <!--可见item数-->
        <attr name="wvVisibleItems" format="integer" />
        <!--当前选中下标-->
        <attr name="wvSelectedItemPosition" format="integer" />
        <!--是否循环滚动-->
        <attr name="wvCyclic" format="boolean" />

        <!--是否显示分割线-->
        <attr name="wvShowDivider" format="boolean" />
        <!--分割线类型-->
        <attr name="wvDividerType" format="enum">
            <enum name="fill" value="0" />
            <enum name="wrap" value="1" />
        </attr>
        <!--分割线颜色-->
        <attr name="wvDividerColor" format="color" />
        <!--分割线高度-->
        <attr name="wvDividerHeight" format="dimension" />
        <!--wrap类型下分割线内边距-->
        <attr name="wvDividerPaddingForWrap" format="dimension" />
        <!--是否是弯曲（3D）效果-->
        <attr name="wvCurved" format="boolean" />
        <!--弯曲（3D）效果左右圆弧偏移效果方向 可向指定方向偏移-->
        <attr name="wvCurvedArcDirection" format="enum">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <!--弯曲（3D）效果左右圆弧偏移效果方向系数 偏移系数-->
        <attr name="wvCurvedArcDirectionFactor" format="float" />
        <!--选中后折射的偏移(弯曲（3D）效果和普通效果都适用) 与字体大小的比值，1为不偏移 越小偏移越明显-->
        <attr name="wvRefractRatio" format="float" />
        <!--是否绘制选中区域-->
        <attr name="wvDrawSelectedRect" format="boolean" />
        <!--选中区域颜色-->
        <attr name="wvSelectedRectColor" format="color" />
        <!--选中区域/分割线上下偏移，通过移动分割线上下限制的位置来实现扩大/缩小显示区域-->
        <attr name="wvDividerOffset" format="dimension" />
    </declare-styleable>

    <!-- 年选择器 -->
    <declare-styleable name="YearWheelView">
        <!-- 开始年份 -->
        <attr name="ywvStartYear" format="integer" />
        <!-- 结束年份 -->
        <attr name="ywvEndYear" format="integer" />
        <!-- 当前选择的年份 -->
        <attr name="ywvSelectedYear" format="integer" />
    </declare-styleable>

    <!-- 月选择器 -->
    <declare-styleable name="MonthWheelView">
        <!-- 当前选择的月份 -->
        <attr name="mwvSelectedMonth" format="integer" />
    </declare-styleable>

    <!-- 天选择器 -->
    <declare-styleable name="DayWheelView">
        <!-- 当前的年份 -->
        <attr name="dwvYear" format="integer" />
        <!-- 当前的月份 -->
        <attr name="dwvMonth" format="integer" />
        <!-- 当前选择的天 -->
        <attr name="dwvSelectedDay" format="integer" />
    </declare-styleable>

    <declare-styleable name="VoiceLineView">
        <!--中间线的颜色，就是波形的时候，大家可以看到，中间有一条直线，就是那个-->
        <attr name="vlvMiddleLine" format="color" />
        <!--中间线的高度，因为宽度是充满的-->
        <attr name="vlvMiddleLineHeight" format="dimension" />
        <!--波动的线的颜色，如果是矩形样式的话，刚是矩形的颜色-->
        <attr name="vlvVoiceLine" format="color" />
        <!--波动线的横向移动速度，线的速度的反比，即这个值越小，线横向移动越快，越大线移动越慢，默认90-->
        <attr name="vlvLineSpeed" format="integer" />
        <!--矩形的宽度-->
        <attr name="vlvRectWidth" format="dimension" />
        <!--矩形之间的间隔-->
        <attr name="vlvRectSpace" format="dimension" />
        <!--矩形的初始高度，就是没有声音的时候，矩形的高度-->
        <attr name="vlvRectInitHeight" format="dimension" />
        <!--所输入音量的最大值-->
        <attr name="vlvMaxVolume" format="float" />
        <!--控件的样式，一共有两种，波形或者矩形-->
        <attr name="vlvViewMode">
            <enum name="line" value="0" />
            <enum name="rect" value="1" />
        </attr>
        <!--灵敏度，默认值是4-->
        <attr name="vlvSensibility">
            <enum name="one" value="1" />
            <enum name="two" value="2" />
            <enum name="three" value="3" />
            <enum name="four" value="4" />
            <enum name="five" value="5" />
        </attr>
        <!--精细度，绘制曲线的时候，每几个像素绘制一次，默认是1，一般，这个值越小，曲线越顺滑，
            但在一些旧手机上，会出现帧率过低的情况，可以把这个值调大一点，在图片的顺滑度与帧率之间做一个取舍-->
        <attr name="vlvFineness">
            <enum name="one" value="1" />
            <enum name="two" value="2" />
            <enum name="three" value="3" />
        </attr>
    </declare-styleable>

    <!-- 轮播图 -->
    <declare-styleable name="BannerView">
        <!-- 未选择状态资源文件 -->
        <attr name="bvUnSelectedDrawable" format="reference" />
        <!-- 选择状态资源文件 -->
        <attr name="bvSelectedDrawable" format="reference" />
        <!--  -->
        <attr name="bvSelectedIndicatorColor" format="color|reference" />
        <attr name="bvUnSelectedIndicatorColor" format="color|reference" />
        <!-- 指示器样式（矩形、椭圆形） -->
        <attr name="bvIndicatorShape" format="enum">
            <!-- 矩形 -->
            <enum name="rect" value="0" />
            <!-- 椭圆形 -->
            <enum name="oval" value="1" />
        </attr>
        <!-- 选择状态指示器高度 -->
        <attr name="bvSelectedIndicatorHeight" format="dimension|reference" />
        <!-- 选择状态指示器宽度 -->
        <attr name="bvSelectedIndicatorWidth" format="dimension|reference" />
        <!-- 默认状态指示器高度 -->
        <attr name="bvUnSelectedIndicatorHeight" format="dimension|reference" />
        <!-- 默认状态指示器宽度 -->
        <attr name="bvUnSelectedIndicatorWidth" format="dimension|reference" />
        <!-- 指示器显示的位置 -->
        <attr name="bvIndicatorPosition" format="enum">
            <enum name="centerBottom" value="0" />
            <enum name="rightBottom" value="1" />
            <enum name="leftBottom" value="2" />
            <enum name="centerTop" value="3" />
            <enum name="rightTop" value="4" />
            <enum name="leftTop" value="5" />
        </attr>
        <!-- 每个指示的间隔距离 -->
        <attr name="bvIndicatorSpace" format="dimension|reference" />
        <!-- 指示器的边缘距离 -->
        <attr name="bvIndicatorMargin" format="dimension|reference" />
        <!-- 轮播时间 -->
        <attr name="bvAutoPlayDuration" format="integer|reference" />
        <!-- 滚动时间 -->
        <attr name="bvScrollDuration" format="integer|reference" />
        <!-- 是否自动轮播 -->
        <attr name="bvIsAutoPlay" format="boolean|reference" />
        <!-- 是否显示指示器 -->
        <attr name="bvIsShowIndicator" format="boolean|reference" />
    </declare-styleable>

    <!-- 状态视图 -->
    <declare-styleable name="LoadLayout">
        <!-- 加载中布局 -->
        <attr name="loadingLayout" format="reference" />
        <!-- 空布局 -->
        <attr name="emptyLayout" format="reference" />
        <!-- 错误布局 -->
        <attr name="errorLayout" format="reference" />
    </declare-styleable>

    <!-- 阴影布局 -->
    <declare-styleable name="ShadowLayout">
        <!--阴影的圆角大小-->
        <attr name="slRadius" format="dimension" />
        <attr name="slTopLeftRadius" format="dimension" />
        <attr name="slTopRightRadius" format="dimension" />
        <attr name="slBottomLeftRadius" format="dimension" />
        <attr name="slBottomRightRadius" format="dimension" />
        <!--阴影的扩散范围(也可以理解为扩散程度)-->
        <attr name="slShadowLimit" format="dimension" />
        <!--阴影的扩散颜色-->
        <attr name="slShadowColor" format="color" />
        https://github.com/HenleyLee/ShadowLayout.git
        <!-- 背景颜色 -->
        <attr name="slBackgroundColor" format="color" />
        <!--x轴的偏移量-->
        <attr name="slOffsetX" format="dimension" />
        <!--y轴的偏移量-->
        <attr name="slOffsetY" format="dimension" />
        <!--左边是否显示阴影-->
        <attr name="slShowLeft" format="boolean" />
        <!--右边是否显示阴影-->
        <attr name="slShowRight" format="boolean" />
        <!--上边是否显示阴影-->
        <attr name="slShowTop" format="boolean" />
        <!--下面是否显示阴影-->
        <attr name="slShowBottom" format="boolean" />
        <!-- 是否一直显示圆角,当隐藏阴影时 -->
        <attr name="slAlwaysShowRadius" format="boolean" />

    </declare-styleable>

    <declare-styleable name="MarqueeView">
        <attr name="mvInterval" format="integer|reference" />
        <attr name="mvAnimDuration" format="integer|reference" />
        <attr name="mvTextSize" format="dimension|reference" />
        <attr name="mvTextColor" format="color|reference" />
        <attr name="mvSingleLine" format="boolean" />
        <attr name="mvGravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>

    <!-- 倒计时 -->
    <declare-styleable name="CountdownView">
        <attr name="cvIncludeFontPadding" format="boolean" />

        <attr name="cvTimeFormat" format="string" />

        <attr name="cvShowDays" format="boolean" />
        <attr name="cvShowHours" format="boolean" />
        <attr name="cvShowMinutes" format="boolean" />
        <attr name="cvShowSeconds" format="boolean" />

        <attr name="cvTimeTextSize" format="dimension" />
        <attr name="cvTimeTextColor" format="color" />
        <attr name="cvIsTimeTextBold" format="boolean" />
        <attr name="cvTimeTextLetterSpacing" format="dimension" />
        <attr name="cvTimeTextLetterBackgroundSpacing" format="dimension" />
        <attr name="cvTimeBackground" format="reference|color" />
        <attr name="cvTimeBackgroundWidth" format="dimension" />
        <attr name="cvTimeBackgroundHeight" format="dimension" />
        <attr name="cvTimeBackgroundPadding" format="dimension" />
        <attr name="cvTimeBackgroundPaddingX" format="dimension" />
        <attr name="cvTimeBackgroundPaddingY" format="dimension" />
        <attr name="cvTimeBackgroundPaddingLeft" format="dimension" />
        <attr name="cvTimeBackgroundPaddingRight" format="dimension" />
        <attr name="cvTimeBackgroundPaddingTop" format="dimension" />
        <attr name="cvTimeBackgroundPaddingBottom" format="dimension" />

        <attr name="cvSuffixDayText" format="string" />
        <attr name="cvSuffixHourText" format="string" />
        <attr name="cvSuffixMinuteText" format="string" />
        <attr name="cvSuffixSecondText" format="string" />
        <attr name="cvSuffixTextSize" format="dimension" />
        <attr name="cvSuffixTextColor" format="color" />
        <attr name="cvIsSuffixTextBold" format="boolean" />
        <attr name="cvSuffixTextMargin" format="dimension" />
        <attr name="cvSuffixBackground" format="reference|color" />
        <attr name="cvSuffixBackgroundWidth" format="dimension" />
        <attr name="cvSuffixBackgroundHeight" format="dimension" />
        <attr name="cvSuffixBackgroundPadding" format="dimension" />
        <attr name="cvSuffixBackgroundPaddingX" format="dimension" />
        <attr name="cvSuffixBackgroundPaddingY" format="dimension" />
        <attr name="cvSuffixBackgroundPaddingLeft" format="dimension" />
        <attr name="cvSuffixBackgroundPaddingRight" format="dimension" />
        <attr name="cvSuffixBackgroundPaddingTop" format="dimension" />
        <attr name="cvSuffixBackgroundPaddingBottom" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MsgView">
        <!-- 圆角矩形背景色 -->
        <attr name="mvBackgroundColor" format="color" />
        <!-- 圆角弧度,单位dp-->
        <attr name="mvCornerRadius" format="dimension" />
        <!-- 圆角弧度,单位dp-->
        <attr name="mvStrokeWidth" format="dimension" />
        <!-- 圆角边框颜色-->
        <attr name="mvStrokeColor" format="color" />
        <!-- 圆角弧度是高度一半-->
        <attr name="mvIsRadiusHalfHeight" format="boolean" />
        <!-- 圆角矩形宽高相等,取较宽高中大值-->
        <attr name="mvIsWidthHeightEqual" format="boolean" />
    </declare-styleable>
    <declare-styleable name="RadarProgressView">

        <!--圆圈和交叉线的颜色-->
        <attr name="rpvCircleColor" format="color" />
        <!--圆圈的数量-->
        <attr name="rpvCircleNum" format="integer" />
        <!--扫描的颜色 RadarView会对这个颜色做渐变透明处理-->
        <attr name="rpvSweepColor" format="color" />
        <!--水滴的颜色-->
        <attr name="rpvRaindropColor" format="color" />
        <!--水滴的数量 这里表示的是水滴最多能同时出现的数量。因为水滴是随机产生的，数量是不确定的-->
        <attr name="rpvRaindropNum" format="integer" />
        <!--是否显示交叉线-->
        <attr name="rpvShowCross" format="boolean" />
        <!--是否显示水滴-->
        <attr name="rpvShowRaindrop" format="boolean" />
        <!--扫描的转速，表示几秒转一圈-->
        <attr name="rpvSpeed" format="float" />
        <!--水滴显示和消失的速度-->
        <attr name="rpvFlicker" format="float" />
    </declare-styleable>

    <declare-styleable name="LabelView">
        <!-- 设置文字内容 -->
        <attr name="lvText" format="string" />
        <!-- 设置文字颜色,默认#ffffff -->
        <attr name="lvTextColor" format="color" />
        <!-- 设置文字大小,默认11sp -->
        <attr name="lvTextSize" format="dimension" />
        <!-- 设置文字是否支持加粗,默认true -->
        <attr name="lvTextBold" format="boolean" />
        <!-- 设置文字是否支持全部大写,默认true-->
        <attr name="lvTextAllCaps" format="boolean" />
        <!-- 设置背景颜色,默认"#FF4081"-->
        <attr name="lvBackgroundColor" format="color" />
        <!-- 设置LabelView所在矩形最小宽高,默认mFillTriangle?35dp:50dp-->
        <attr name="lvMinSize" format="dimension" />
        <!-- 设置文字上下padding,默认3.5dp,mFillTriangle为true时无效-->
        <attr name="lvPadding" format="dimension" />
        <!-- 设置LabelView方向 -->
        <attr name="lvGravity" format="enum">
            <enum name="TOP_LEFT" value="51" />
            <enum name="TOP_RIGHT" value="53" />
            <enum name="BOTTOM_LEFT" value="83" />
            <enum name="BOTTOM_RIGHT" value="85" />
        </attr>
        <!-- 设置是否填充三角区域,默认false -->
        <attr name="lvFillTriangle" format="boolean" />
        <!-- 设置是否浮嵌显示大小 -->
        <attr name="lvFillSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CommonTitleBar">
        <!-- 标题栏背景 图片或颜色 -->
        <attr name="titleBarColor" format="color" />
        <!-- 填充状态栏 -->
        <attr name="fillStatusBar" format="boolean" />
        <!-- 状态栏颜色 -->
        <attr name="statusBarColor" format="color" />
        <!-- 状态栏模式 -->
        <attr name="statusBarMode" format="enum">
            <enum name="dark" value="0" />
            <enum name="light" value="1" />
        </attr>
        <!-- 标题栏高度 -->
        <attr name="titleBarHeight" format="dimension" />
        <!-- 显示标题栏分割线 -->
        <attr name="showBottomLine" format="boolean" />
        <!-- 标题栏分割线颜色 -->
        <attr name="bottomLineColor" format="color" />
        <!-- 底部阴影高度 showBottomLine = false时有效 -->
        <attr name="bottomShadowHeight" format="dimension" />

        <!-- 左边视图类型 -->
        <attr name="leftType">
            <enum name="none" value="0" />
            <enum name="textView" value="1" />
            <enum name="imageButton" value="2" />
            <enum name="customView" value="3" />
        </attr>
        <!-- TextView 文字, 对应leftType_TextView -->
        <attr name="leftText" format="string" />
        <!-- TextView 颜色, 对应leftType_TextView -->
        <attr name="leftTextColor" format="color" />
        <!-- TextView 字体大小, 对应leftType_TextView -->
        <attr name="leftTextSize" format="dimension" />
        <!-- TextView 左边图片, 对应leftType_TextView -->
        <attr name="leftDrawable" format="reference" />
        <!-- TextView 左边padding, 对应leftType_TextView -->

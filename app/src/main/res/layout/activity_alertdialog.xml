<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:onClick="onTipDialogClick"
        android:text="TipDialogClick"/>
    <Button
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:onClick="onTitleTipDialogClick"
        android:text="TitleTipDialogClick"/>
    <Button
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:onClick="onSelectDialogClick"
        android:text="SelectDialogClick"/>
    <Button
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:onClick="onTitleSelectDialogClick"
        android:text="TitleSelectDialogClick"/>
    <Button
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:onClick="onCustomDialogClick"
        android:text="CustomDialogClick"/>

    <Button
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:onClick="onYMDDialogClick"
        android:text="年月日选择框"/>

    <Button
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:onClick="onYMDialogClick"
        android:text="年月选择框"/>

    <Button
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:onClick="onYDialogClick"
        android:text="年选择框"/>


    <Button
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:onClick="onPickerDialogClick"
        android:text="联动选择框"/>


</LinearLayout>